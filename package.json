{
  "name": "nodesphere",
  "version": "0.6.1",
  "description": "graph interoperability",
  "main": "lib",
  "scripts": {
    "build": "webpack",
    "link-all": "npm link",
    "clean": "rm -rf ./lib/",
    "compile": "coffee --output lib/ --compile src/",
    "watch": "coffee --watch --output lib/ --compile src/",
    "prepublish": "in-publish && npm run compile && npm run build || not-in-publish",
    "shipit": "npm test && git push origin head",
    "test": "npm run compile && mocha",
    "pub": "npm run examples:publish",
    "examples:publish": "npm-run-all --silent examples publish:gh-pages publish:gh-pages:notify",
    "examples": "npm-run-all --silent build copy-dist-to-examples",
    "copy-dist-to-examples": "cp dist/nodesphere.js examples/",
    "publish:gh-pages": "gh-pages --dist examples --message 'Update gh-pages [ci skip]'",
    "publish:gh-pages:notify": "echo http://nodesphere.github.io/nodesphere",
    "ipfs": "open https://ipfs.io/ipfs/`~/gocode/bin/ipfs add -r -q examples | tail -n 1`",
    "preview": "http-server examples -c-1 -p 1234"
  },
  "author": "Harlan T Wood (https://github.com/harlantwood)",
  "contributors": [
    "Jack Senechal (https://github.com/jacksenechal)",
    "Connor Turland (https://github.com/Connoropolous)"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.4.7",
    "ipfs-api": "^12.1.2",
    "json-stable-stringify": "^1.0.1",
    "lightsaber": "^0.6.8",
    "lodash": "^4.17.4",
    "native-promisify-if-present": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.12.2",
    "gh-pages": "^0.12.0",
    "https-browserify": "0.0.1",
    "in-publish": "^2.0.0",
    "ipfsd-ctl": "^0.18.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "stream-http": "^2.6.0",
    "webpack": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nodesphere/nodesphere.git"
  },
  "keywords": [
    "content-addressable",
    "edge",
    "graph",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/nodesphere/nodesphere/issues"
  }
}
