{
  "name": "nodesphere",
  "version": "0.3.2",
  "description": "Simple graph storage designed for universal interoperability",
  "main": "lib/exports.js",
  "scripts": {
    "clean": "rm -rf ./lib/",
    "compile": "./node_modules/.bin/coffee --output lib/ --compile src/",
    "prepublish": "npm run compile",
    "serve-dev": "./node_modules/.bin/nodemon --ext coffee,js --ignore tmp/ lib/server/server.js",
    "serve-production": "node lib/server/server.js",
    "serve": "npm run serve-dev",
    "start": "npm run serve-production",
    "test": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register",
    "watch": "./node_modules/.bin/coffee --watch --output lib/ --compile src/"
  },
  "author": "Harlan T Wood <code@nodesphere.org> (http://nodesphere.org/)",
  "contributors": [
    "Jack Senechal (https://github.com/jacksenechal)",
    "Chris Larcombe (http://chrislarcombe.com)"
  ],
  "license": "ISC",
  "engines": {
    "node": "0.10.31",
    "npm": "^2.5"
  },
  "bin": {
    "nodesphere": "bin/nodesphere",
    "nodesphere-multiply": "bin/nodesphere-multiply"
  },
  "dependencies": {
    "body-parser": "^1.8.1",
    "commander": "^2.5.0",
    "crawler": "^0.4.1",
    "express": "^4.9.4",
    "glob": "^4.0.6",
    "http-request": "^0.6.3",
    "lightsaber": "^0.4.0",
    "newrelic": "^1.12.0",
    "node-forge": "^0.6.16",
    "socket.io": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "mocha": "^2.0.1",
    "nodemon": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodesphere/nodesphere.git"
  },
  "keywords": [
    "blockchain",
    "content-addressable",
    "edge",
    "graph",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/nodesphere/nodesphere/issues"
  }
}
